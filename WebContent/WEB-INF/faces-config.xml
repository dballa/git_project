</navigation-case>

		<navigation-case>
<from-action>#{loginManagedBean.login}</from-action>
<from-outcome>User</from-outcome>
<to-view-id>/User.xhtml</to-view-id>
<redirect></redirect>
</navigation-case>
	
	</navigation-rule>
</faces-config>
